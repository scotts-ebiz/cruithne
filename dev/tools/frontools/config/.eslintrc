env:
  node: true
  browser: true
  amd: true
  es6: true
  jquery: true
rules:
  indent: ["error", "tab"]
  brace-style: [2, "stroustrup"]
  comma-style: [2, "last"]
  default-case: 2
  func-style: [1, "expression"]
  no-floating-decimal: 2
  no-nested-ternary: 2
  no-undefined: 2
  radix: 2
  space-before-function-paren: [2, "never"]
  space-after-keywords: [2, "always"]
  space-before-blocks: 2
  spaced-comment: [0, "always", { exceptions: ["-"] }]
  valid-jsdoc: [0, { requireReturn: false, prefer: { return: "returns" }}]
  wrap-iife: [2, "inside"]
  guard-for-in: 1
  strict: [1, "global"]
  global-strict: 0
  camelcase: 1
  curly: [2, "all"]
  eqeqeq: [2, "allow-null"]
  no-empty: 2
  no-underscore-dangle: 0
  no-use-before-define: 1
  no-obj-calls: 2
  no-unused-vars: [0, {"vars": "local", "args": "after-used"}]
  new-cap: 2
  no-shadow: 1
  no-invalid-regexp: 2
  comma-dangle: [2, "never"]
  no-undef: 2
  no-new: 2
  no-extra-semi: 2
  no-caller: 1
  semi: 2
  quotes: [2, "single"]
  no-unreachable: 2
  eol-last: 2
  no-alert: 1
  no-debugger: 1
  no-console: 1
globals:
  jQuery: false
  $j: true
