{
    "name": "magento/project-enterprise-edition",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "license": [
        "proprietary"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "akeneo/magento2-connector-enterprise": "101.*",
        "amasty/securityauth": "^1.1",
        "avalara/avatax-magento": "^2.2.2",
        "composer/composer": "@alpha",
        "cweagans/composer-patches": "^1.6",
        "gigya/magento2-im": "5.6.1",
        "magento/composer-root-update-plugin": "^1.1",
        "magento/product-enterprise-edition": ">=2.3.6 <2.3.7",
        "magepal/magento2-googletagmanager": "^2.6",
        "mageplaza/module-smtp": "^4.0",
        "recurly/recurly-client": "^2.12",
        "snowdog/frontools": "^1.11",
        "zaius/zaius-magento-2": "^1.0.7",
        "dotmailer/dotmailer-magento2-extension": "4.5.1 as 4.4.1-p1"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.2.3",
        "friendsofphp/php-cs-fixer": "~2.18.2",
        "lusitanian/oauth": "~0.8.11",
        "magento/magento-coding-standard": "~6.0.0",
        "magento/magento2-functional-testing-framework": "~2.6.5",
        "pdepend/pdepend": "2.8.0",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "^3.0.0"
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 1 ] && vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 1 ] && vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard/"
        ]
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\ToolkitFramework\\": "dev/tools/performance-toolkit/framework/Magento/ToolkitFramework/"
        }
    },
    "prefer-stable": true,
    "version": "2.3.1",
    "minimum-stability": "alpha",
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "amasty": {
            "type": "composer",
            "url": "https://composer.amasty.com/enterprise/"
        },
        "gigya-im": {
            "type": "git",
            "url": "https://github.com/gigya/Magento-2"
        },
        "magento2-connector-enterprise": {
            "type": "vcs",
            "url": "ssh://git@distribution.akeneo.com:443/magento2-connector-enterprise"
        }
    },
    "extra": {
        "magento-force": "override",
        "composer-exit-on-patch-failure": true,
        "patches": {
        	"zaius/zaius-magento-2": {
        		"Zaius Duplicate Entries": "patches/composer/zaius_duplicate_patch.diff"
        	}
        }
    },
    "conflict": {
        "gene/bluefoot": "*"
    }
}
